# Git LFS - Track large binary files
vendor/firecracker/firecracker filter=lfs diff=lfs merge=lfs -text
vendor/firecracker/jailer filter=lfs diff=lfs merge=lfs -text
vendor/kernels/vmlinux.bin filter=lfs diff=lfs merge=lfs -text
vendor/rootfs/alpine-minirootfs.tar.gz filter=lfs diff=lfs merge=lfs -text

# Don't track generated rootfs with LFS
/var/lib/firecracker-workspaces/rootfs/*.ext4 filter=lfs diff=lfs merge=lfs -text
